СOM1с=Новый COMObject("V82.COMConnector");
ВнешняяБаза  =  СOM1с.Connect("Srvr=192.168.151.5;Ref=uat;Usr=""****"";Pwd=****");
Запрос   =  ВнешняяБаза.NewObject("Запрос");
Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 20 Наименование ИЗ Справочник.Контрагенты";

Результат   =  Запрос.Выполнить();
Выборка     =  Результат.Выбрать();

Пока Выборка.Следующий() Цикл
	Сообщить(Выборка.Наименование);
КонецЦикла;


ВнешняяБаза = Неопределено;
